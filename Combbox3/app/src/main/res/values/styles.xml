<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
    </style>

    <declare-styleable name="Combobox">
        <attr name="value" format="string" />
        <attr name="displayField" format="string" />
        <attr name="dataMode" format="integer">
            <enum name="json" value="0" />
            <enum name="string" value="1" />
        </attr>
        <attr name="dataStore" format="reference" />
    </declare-styleable>
</resources>
